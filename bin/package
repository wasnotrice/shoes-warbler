#!/usr/bin/env ruby
$LOAD_PATH.unshift(File.expand_path "../../lib", __FILE__)

require 'rubygems'
require 'yaml'
require 'warbler'
require 'warbler/traits/shoes'

module Shoes
  module Swt
    module Package
      class Jar
        def initialize
          @jar = Warbler::Jar.new
        end

        def apply(config)
          @jar.apply(config)
        end

        def create(config)
          @jar.create(config)
        end
      end
    end
  end
end

jar = Shoes::Swt::Package::Jar.new
config = Warbler::Config.new
jar.apply(config)
jar.create(config)

